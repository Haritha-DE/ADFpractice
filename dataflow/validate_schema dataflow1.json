{
	"name": "validate_schema dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EmployeeDelimitedText1",
						"type": "DatasetReference"
					},
					"name": "empsource1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "outputdelimitedtxt",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          empid as string,",
				"          name as string,",
				"          country as string,",
				"          department as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> empsource1",
				"empsource1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['validateschema'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}